{
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": [
    "^node:(.*)$",
    "^@action/core/(.*)$",
    "^@action/github/(.*)$",
    "^@action/exec/(.*)$",
    "^@action/glob/(.*)$",
    "^@action/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
